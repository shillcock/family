= simple_form_for post, remote: false, authenticity_token: true, html: { multipart: true }  do |f|
  = f.error_notification
  = f.input :content, label: false, placeholder: "Whats on your mind?", input_html: { rows: "5" }
  = f.input :photos, as: :file, input_html: { multiple: true, name: "post[photos_attributes][][image]" }
  = f.button :submit, data: { disable_with: "Submitting..." }

